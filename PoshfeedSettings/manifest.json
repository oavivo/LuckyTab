{
	"name": "Poshfeed",
	"version": "0.2",
	"description": "Get something inspiring from around the web every time you open a new tab.",
	"homepage_url": "http://www.poshfeed.com",
	"options_page": "options.html",
	"chrome_url_overrides": {
		"newtab": "front.html"
	},
	"icons": { 
		"16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "background": {
	    "scripts": ["background.js"],
	    "persistent": false
	  },
	"permissions": [
		"topSites",
		"chrome://favicon/",		
		"http://82.196.3.219/",
		"storage"
	],
	"manifest_version": 2	
}
